generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Chat {
  chat_id    Int        @id
  data_start DateTime   @db.Time(6)
  data_send  DateTime[] @db.Time
  text       String     @db.VarChar(128)
  status     String     @db.VarChar(15)
}

model Consultation {
  consultation_id Int      @id
  date_time       DateTime @db.Timestamptz(6)
  amount          Int?
  meeting_link    Json?    @db.Json
  status          Boolean
}

model Image {
  image_id        Int     @id
  profile_picture String? @db.VarChar(400)
  profile_pharm_  String  @map("profile_pharm ") @db.VarChar(400)
  image_med       String? @db.VarChar(400)
}

model ListRecipe {
  listrecipe_id Int      @id
  assigment     String   @db.VarChar(200)
  data          DateTime @db.Date
}

model Marking {
  marking_id         Int    @id
  dose_              String @map("dose ") @db.VarChar(45)
  side_effects_      String @map("side_effects ") @db.VarChar(45)
  contraindications_ String @map("contraindications ") @db.VarChar(200)
}

model Medication {
  medication_id_ Int      @id @map("medication_id ")
  name           String   @db.VarChar(150)
  data_storage_  DateTime @map("data_storage ") @db.Date
  composition    String   @db.VarChar(500)
  application_   String   @map("application ") @db.VarChar(500)
  status         String   @db.VarChar(20)
}

model Pharmacist {
  harmacist_id  Int    @id @map("Ñ€harmacist_id")
  full_name     String @db.VarBit(100)
  gender        String @db.VarChar(10)
  qualification String @db.VarChar(200)
  email         String @db.VarChar(125)
  password      Int
  contact       String @db.VarChar(50)
  raiting       Int?
}

model Pharmacy {
  pharmacy_id Int    @id
  name        String @db.VarChar(150)
  address     String @db.VarChar(150)
}

model Recipe {
  recipe_id      Int      @id
  date_discharge DateTime @db.Date
  dose           String   @db.VarChar(50)
  suitability_   DateTime @map("suitability ") @db.Date
  status         String   @db.VarChar(25)
  entity         Int
}

model User {
  user_id   Int      @id
  full_name String   @db.VarChar(100)
  gender    String   @db.VarChar(10)
  email     String   @db.VarChar(125)
  password  Int
  address   String?  @db.VarChar(125)
  phone_num String   @db.VarChar(12)
  age       DateTime @db.Date
}
